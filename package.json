{
    "name": "sushi-vscode",
    "displayName": "sushi-vscode",
    "description": "Sushis scrolling on your status bar.",
    "version": "0.0.1",
    "publisher": "espresso3389",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Sushi"
    ],
    "icon": "sushi.png",
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/espresso3389/sushi-vscode/issues"
    },
    "homepage": "https://github.com/espresso3389/sushi-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/espresso3389/sushi-vscode.git"
    },
    "activationEvents": [
        "onCommand:extension.sushiOnStatusBar"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sushiOnStatusBar",
            "title": "It's Sushi Time!"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}