{
  "name": "avocado-vscode",
  "displayName": "avocado-vscode",
  "description": "May the Avocado be with you.",
  "version": "0.0.3",
  "publisher": "espresso3389",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Avocado"
  ],
  "icon": "avocado.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/espresso3389/avocado-vscode/issues"
  },
  "homepage": "https://github.com/espresso3389/avocado-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/espresso3389/avocado-vscode.git"
  },
  "activationEvents": [
    "onCommand:extension.avocadoOnStatusBar"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.avocadoOnStatusBar",
        "title": "It's Avocado Time!"
      },
      {
        "command": "extension.needMoreAvocado",
        "title": "Need more Avocado!"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.10",
    "vscode": "^1.0.3",
    "mocha": "^3.2.0",
    "@types/node": "^0.0.0",
    "@types/mocha": "^2.2.33"
  },
  "dependencies": {
    "openurl": "^1.1.1"
  }
}
